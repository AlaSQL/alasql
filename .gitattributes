# Auto detect text files and perform LF normalization

*        text=auto

*.cs     text diff=csharp eol=lf
*.java   text diff=java eol=lf
*.html   text diff=html eol=lf
*.css    text eol=lf
*.js     text eol=lf
*.sql    text eol=lf
*.php    text eol=lf
*.md    text eol=lf
*.json    text eol=lf
*.csv    text eol=lf
*.txt    text eol=lf
*.java    text eol=lf
*.g4    text eol=lf
*.shar    text eol=lf
*.ypp    text eol=lf
*.jison    text eol=lf
*.yaml    text eol=lf
*.yml    text eol=lf

*.csproj text merge=union eol=lf
*.sln    text merge=union eol=crlf

*.xlsx binary
*.xls binary

alasql.js binary merge=ours
alasql.js.map binary merge=ours
alasql.min.js binary merge=ours


# absolute paths are ok, as are globs
#/**/postinst* text eol=lf

# paths that don't start with / are relative to the .gitattributes folder
#relative/path/*.txt text eol=lf


